{
    "AWS": {
        "CertificateManager": {
            "CertificateArn": "OVERRIDE_ME"
        },
        "RDS": {
            "MySQL": {
                "AllocatedStorage": 50,
                "Backup": {
                    "PreferedWindow": "04:00-05:00",
                    "RetentionDays": 7
                },
                "InstanceClass": "BURSTABLE3",
                "InstanceSize": "MICRO",
                "MasterUser": {
                    "PasswordLength": 20,
                    "Username": "root"
                },
                "Port": 3306,
                "PreferedMaintenanceWindow": "Sun:05:00-Sun:06:00",
                "RemovalPolicy": "DESTROY",
                "StorageEncrypted": true
            }
        },
        "Route53": {
            "DomainName": "OVERRIDE_ME"
        }
    },
    "JuiceShop": {
        "ContainerImage": "bkimminich/juice-shop",
        "DesiredCount": 1,
        "MemoryLimitMiB": 512,
        "SubdomainName" : "juiceshop"
    },
    "Nextcloud": {
        "AdminUser": {
            "PasswordLength": 20,
            "Username": "admin"
        },
        "ContainerImage": "nextcloud",
        "DatabaseName": "nextcloud",
        "DesiredCount": 1,
        "MemoryLimitMiB": 1024,
        "SubdomainName" : "nextcloud"
    }
}